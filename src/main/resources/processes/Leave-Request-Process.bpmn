            <camunda:validation />
          </camunda:formField>
          <camunda:formField id="endDate" label="วันสิ้นสุดการลา" type="date" />
          <camunda:formField id="reason" label="เหตุผล" type="string" />
        </camunda:formData>
      </bpmn:extensionElements>
      <bpmn:incoming>Flow_0kmbaju</bpmn:incoming>
      <bpmn:outgoing>Flow_0xkhr7e</bpmn:outgoing>
    </bpmn:userTask>
    <bpmn:sequenceFlow id="Flow_0kmbaju" sourceRef="startLeaveRequest" targetRef="submitLeaveRequest" />
    <bpmn:endEvent id="Event_18u3fgx">
