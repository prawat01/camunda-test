    <bpmn:sequenceFlow id="Flow_0xkhr7e" sourceRef="submitLeaveRequest" targetRef="approveByManager" />
    <bpmn:sequenceFlow id="Flow_096qoth" sourceRef="approveByManager" targetRef="Gateway_1jz7t5l" />
    <bpmn:sequenceFlow id="flowRejected" name="ปฏิเสธ" sourceRef="Gateway_1jz7t5l" targetRef="sendRejectionEmail">
      <bpmn:conditionExpression xsi:type="bpmn:tFormalExpression">${approved == false}</bpmn:conditionExpression>
    </bpmn:sequenceFlow>
    <bpmn:sequenceFlow id="flowApproved" name="อนุมัติ" sourceRef="Gateway_1jz7t5l" targetRef="sendApprovalEmail">
      <bpmn:conditionExpression xsi:type="bpmn:tFormalExpression">${approved == true}</bpmn:conditionExpression>
    </bpmn:sequenceFlow>
    <bpmn:sequenceFlow id="Flow_0rd6upb" sourceRef="sendApprovalEmail" targetRef="saveLeaveRecord" />
    <bpmn:sequenceFlow id="Flow_1snifl4" sourceRef="saveLeaveRecord" targetRef="Event_18u3fgx" />
