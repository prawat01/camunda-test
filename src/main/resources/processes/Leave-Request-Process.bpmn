      <bpmn:outgoing>Flow_0rd6upb</bpmn:outgoing>
    </bpmn:serviceTask>
    <bpmn:exclusiveGateway id="Gateway_1jz7t5l">
      <bpmn:incoming>Flow_096qoth</bpmn:incoming>
      <bpmn:outgoing>flowRejected</bpmn:outgoing>
      <bpmn:outgoing>flowApproved</bpmn:outgoing>
    </bpmn:exclusiveGateway>
    <bpmn:serviceTask id="sendRejectionEmail" name="ส่งอีเมลปฏิเสธ" camunda:delegateExpression="${sendEmailDelegate}">
